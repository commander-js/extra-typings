    ]
  },
  "peerDependencies": {
    "commander": "~12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "^20.11.7",
    "commander": "~12.0.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
